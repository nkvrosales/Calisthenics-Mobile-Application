<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearLayout2">
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:id="@+id/scrollView1">

        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="1110px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout1">
            <TextView
                android:text="*Note: Use the rest timer button every 1 set"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView7" />
            <TextView
                android:text="Wall Push-up: 3 sets 50 reps"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/textView1" />
            <ImageView
                android:src="@drawable/wallPushUps"
                android:layout_width="match_parent"
                android:layout_height="112.5dp"
                android:id="@+id/imageView1" />
            <TextView
                android:text="Australlian Rows: 3 sets 15 reps"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/textView2" />
            <ImageView
                android:src="@drawable/ausRows"
                android:layout_width="match_parent"
                android:layout_height="112.5dp"
                android:id="@+id/imageView2" />
            <TextView
                android:text="Body Weight Squats: 3 sets 10 reps"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/textView3" />
            <ImageView
                android:src="@drawable/squats"
                android:layout_width="match_parent"
                android:layout_height="112.5dp"
                android:id="@+id/imageView3" />
            <TextView
                android:text="Jumping Jacks: 3 sets 30 secs "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/textView4" />
            <ImageView
                android:src="@drawable/jumpingjacks"
                android:layout_width="match_parent"
                android:layout_height="112.5dp"
                android:id="@+id/imageView4"/>
            <TextView
                android:text="Crunches: 3 sets 10 reps"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/textView5" />
            <ImageView
                android:src="@drawable/crunches"
                android:layout_width="match_parent"
                android:layout_height="112.5dp"
                android:id="@+id/imageView5" />
        </LinearLayout>
    </ScrollView>
    <TextView
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:gravity="center"
        android:textSize="60dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6" />

    <Button
        android:text="Rest Timer"
        android:layout_gravity="center"
        android:layout_width="395dp"
        android:layout_height="50dp"
        android:textColor="@android:color/white"
        android:backgroundTint="@android:color/black"
        android:id="@+id/button3" />
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:id="@+id/linearLayout3">


        <Button
            android:text="Give up"
            android:layout_width="195dp"
            android:layout_height="match_parent"
            android:layout_marginRight="5dp"
            android:textColor="@android:color/white"
            android:backgroundTint="@android:color/black"
            android:id="@+id/button1" />
        <Button
            android:text="Finish"
            android:layout_width="195dp"
            android:layout_height="match_parent"
            android:textColor="@android:color/white"
            android:backgroundTint="@android:color/black"
            android:id="@+id/button2" />
    </LinearLayout>
</LinearLayout>